<PreferenceGroup
    xmlns:camera="http://schemas.android.com/apk/res-auto">

    <com.google.android.cameraview.demo.camera.data.CamListPreference
        camera:key="com.google.android.cameraview.demo.camera.module.VideoModule"
        camera:title="@string/mode_video"/>

    <com.google.android.cameraview.demo.camera.data.CamListPreference
        camera:key="com.google.android.cameraview.demo.camera.module.PhotoModule"
        camera:title="@string/mode_photo"/>

    <com.google.android.cameraview.demo.camera.data.CamListPreference
        camera:key="com.google.android.cameraview.demo.camera.module.ProfessionalModule"
        camera:title="@string/mode_professional"/>

    <com.google.android.cameraview.demo.camera.data.CamListPreference
        camera:key="com.google.android.cameraview.demo.camera.module.DualCameraModule"
        camera:title="@string/mode_dual_camera"/>
</PreferenceGroup>